{
  "name": "myapp-cli",
  "version": "1.0.0",
  "description": "The Command Line Interface (CLI) for MyApp",
  "main": "myApp.js",
  "scripts": {
    "start": "node myApp.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "init": "node myApp.js init --all",
